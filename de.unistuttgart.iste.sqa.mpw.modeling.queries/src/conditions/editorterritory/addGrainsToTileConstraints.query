context Territory::addGrainsToTile(location:Location, amount:EInt)

precondition: 
    amount > 0;

precondition: 
    self.isLocationInTerritory(location);

precondition:
    self.getTileAt(location).contents->isEmpty();

postcondition: 
    self.getTileAt(location).contents->typeSelect(Grain)->size() = amount;
    