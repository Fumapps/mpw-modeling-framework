<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="command" nsURI="http://www.hamster.uni-stuttgart.de/command"
    nsPrefix="command">
  <eClassifiers xsi:type="ecore:EClass" name="BasicCommand" abstract="true">
    <eOperations name="execute"/>
    <eOperations name="undo"/>
    <eOperations name="redo"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="target" lowerBound="1"
        eType="#//CommandEntity"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="SetPropertyCommand" eSuperTypes="#//BasicCommand"/>
  <eClassifiers xsi:type="ecore:EClass" name="AddEntityCommand" eSuperTypes="#//BasicCommand">
    <eStructuralFeatures xsi:type="ecore:EReference" name="entityToAdd" lowerBound="1"
        eType="#//CommandEntity"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="RemoveEntityCommand" eSuperTypes="#//BasicCommand">
    <eStructuralFeatures xsi:type="ecore:EReference" name="entityToRemove" lowerBound="1"
        eType="#//CommandEntity"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="CommandEntity">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Represents an Entity which is part of the game state model. Any modification on its property shall be tracked via basic-commands."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="commandStack" lowerBound="1"
        eType="#//CommandStack"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="CommandStack">
    <eStructuralFeatures xsi:type="ecore:EReference" name="stack" upperBound="-1"
        eType="#//BasicCommand" containment="true"/>
  </eClassifiers>
</ecore:EPackage>
